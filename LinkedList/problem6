import java.util.Scanner;

class Node {
    int value;
    Node next;
    Node prev;

    Node(int value) {
        this.value = value;
    }
}


class List {
    Node dummy;

    List() {
        dummy = new Node(-1);
        dummy.next = dummy;
        dummy.prev = dummy;
    }

    void print() {
        for (Node x = dummy.next; x != dummy; x = x.next) {
            System.out.print(x.value + " ");
        }
        System.out.println();
    }

    void printReverse() {
        for (Node x = dummy.prev; x != dummy; x = x.prev) {
            System.out.print(x.value + " ");
        }
        System.out.println();
    }

    Node find(int value) {
        for (Node x = dummy.next; x != dummy; x = x.next) {
            if (x.value == value) {
                return x;
            }
        }
        return dummy;
    }

    Node insertAfter(Node x, int value) {
        // value を値にもつ新しいノード newNode を作成


        // x の次のノードの prev を newNode に更新
        // newNode の next を x の次のノードに更新
        // newNode の prev を x に更新
        // x の next を newNode に更新
        // newNode を返す
        return newNode;
    }
}


public class Main {

    public static void main(String[] args) {
        // 新しいリストを作成
        List list = new List();
        // 値 1 をもつノードを挿入
        Node node1 = list.insertAfter(list.dummy, 1);
        // 値 2 をもつノードを挿入
        Node node2 = list.insertAfter(list.dummy, 2);
        // 値 3 をもつノードを挿入
        Node node3 = list.insertAfter(list.dummy, 3);

        // リストを出力
        list.print();
        // 逆向きにも出力
    }
}

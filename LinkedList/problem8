import java.util.Scanner;

class Node {
    int value;
    Node next;
    Node prev;

    Node(int value) {
        this.value = value;
    }
}


class List {
    Node dummy;

    List() {
        dummy = new Node(-1);
        dummy.next = dummy;
        dummy.prev = dummy;
    }

    void print() {
        for (Node x = dummy.next; x != dummy; x = x.next) {
            System.out.print(x.value + " ");
        }
        System.out.println();
    }

    void printReverse() {
        for (Node x = dummy.prev; x != dummy; x = x.prev) {
            System.out.print(x.value + " ");
        }
        System.out.println();
    }

    Node find(int value) {
        for (Node x = dummy.next; x != dummy; x = x.next) {
            if (x.value == value) {
                return x;
            }
        }
        return dummy;
    }

    Node insertAfter(Node x, int value) {
        Node newNode = new Node(value);
        x.next.prev = newNode;
        newNode.next = x.next;
        newNode.prev = x;
        x.next = newNode;
        return newNode;
    }

    Node insertBefore(Node x, int value) {
        Node newNode = new Node(value);
        x.prev.next = newNode;
        newNode.prev = x.prev;
        newNode.next = x;
        x.prev = newNode;
        return newNode;
    }

    void erase(Node x) {
        // もし x がダミーノードであれば
        if (x == dummy){
            return;
        }
        // エラーを出力
        System.out.println("error");
        // x の次のノードの prev を x の前のノードに更新
        // x の前のノードの next を x の次のノードに更新
    }

}


public class Main {

    public static void main(String[] args) {
        List list = new List();
        Node node1 = list.insertAfter(list.dummy, 1);
        Node node2 = list.insertAfter(node1, 2);
        Node node3 = list.insertAfter(node2, 3);

        list.print();
        list.printReverse();

        // node2 を削除

        // リストを出力
        // 逆向きに出力
    }
}

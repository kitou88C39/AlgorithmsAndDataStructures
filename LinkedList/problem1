import java.util.Scanner;

class Node {
	int value;
	Node next;

	Node(int value) {
		this.value = value;
	}
}


class List {
	Node dummy;

	List() {
		dummy = new Node(-1);
		dummy.next = dummy;
	}

	void print() {
		// ここにコードを書く
	}
}


public class Main {

	public static void main(String[] args) {
		List list = new List();
		Node node1 = new Node(1);
		Node node2 = new Node(2);
		Node node3 = new Node(3);

		list.dummy.next = node1;
		node1.next = node2;
		node2.next = node3;
		node3.next = list.dummy;

		list.print();
	}
}

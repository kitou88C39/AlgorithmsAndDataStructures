import java.util.*;

public class Main {

    static int hash(int x, int mod) {
        return x % mod;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int mod = sc.nextInt();

        for (int i = 0; i < n; i++) {
            int x = sc.nextInt();
            System.out.println(hash(x, mod));
        }
    }
}

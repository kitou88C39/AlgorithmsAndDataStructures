import java.util.*;

public class Main {

    static int H(String x){
        return count('p', x) + count('a', x) + count('i', x) + count('z', x);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        for (int i = 0; i < n; i++){
            String x = sc.next()
        }

    }
}
